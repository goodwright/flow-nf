{
    "inputs": {
        "genome_options": {
            "name": "File options",
            "description": "The files needed for demultiplexing.",
            "properties": {
                "samplesheet": {
                    "name": "Annotation",
                    "type": "file",
                    "pattern": "xlsx|csv",
                    "required": true,
                    "description": "Sample annotation sheet."
                },
                "fastq": {
                    "name": "FASTQ",
                    "type": "file",
                    "pattern": "fq\\.gz$|fastq\\.gz$|fq$|fastq$",
                    "required": true,
                    "genome_file": "gtf",
                    "description": "A multiplexed FASTQ file to demultiplex."
                }
            }
        }
    },
    "outputs": [
        {
            "name": "FASTQ Files",
            "description": "The demultiplexed FASTQ files.",
            "filetype": "fastq.gz",
            "process": "ULTRAPLEX"
        }
    ]
}